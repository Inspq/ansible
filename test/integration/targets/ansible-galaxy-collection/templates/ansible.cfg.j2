[galaxy]
server_list=pulp_v2,pulp_v3,galaxy_ng

[galaxy_server.pulp_v2]
url={{ pulp_v2_server }}
username={{ pulp_user }}
password={{ pulp_password }}

[galaxy_server.pulp_v3]
url={{ pulp_v3_server }}
v3=true
username={{ pulp_user }}
password={{ pulp_password }}

[galaxy_server.galaxy_ng]
url={{ galaxy_ng_server }}
token={{ galaxy_ng_token.json.token }}
