<comps>
  <group>
    <id>customgroup</id>
    <name>Custom Group</name>
    <description></description>
    <default>false</default>
    <uservisible>true</uservisible>
    <display_order>1024</display_order>
    <packagelist>
      <packagereq type="mandatory">dinginessentail</packagereq>
    </packagelist>
  </group>

  <group>
    <id>customenvgroup</id>
